    <validationRules>
        <fullName>ValidateClosedOppReqFieldsNotEditable</fullName>
        <active>false</active>
        <description>Checks to make sure that if required fields on the Opportunity page are not Editable by anyone but the Sys Admin.</description>
        <errorConditionFormula>( ISCHANGED( OwnerId ) ||
ISCHANGED( Name ) ||
ISCHANGED( AccountId ) ||
ISCHANGED( CloseDate ) ||
AND( NOT(ISBLANK( Closed_Opp_Date_Time__c )), ISCHANGED( StageName ) ) )
&amp;&amp; NOT($Profile.Name = &quot;System Administrator&quot;)</errorConditionFormula>
        <errorMessage>Opportunity Name, Account Name, Close Date, and Stage changes cannot be saved on a Closed Opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Description_Save_Authorized</fullName>
        <active>true</active>
        <description>Checks to make sure that if the Description field is changed on a Partial Read-only page, that it&apos;s the owner.</description>
        <errorConditionFormula>ISCHANGED(Description) &amp;&amp; NOT($Profile.Name = &quot;System Administrator&quot;) &amp;&amp; ( $User.Id &lt;&gt; Owner.Id )</errorConditionFormula>
        <errorDisplayField>Description</errorDisplayField>
        <errorMessage>Only the Owner can change the Description field on a Closed Opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Verify_Product_Count_on_Closed_Opp</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISCHANGED( Product_Count__c ) ,NOT(ISBLANK( Closed_Opp_Date_Time__c )), NOT($Profile.Name = &quot;System Administrator&quot;))</errorConditionFormula>
        <errorMessage>Cannot modify Products after an Opportunity is Closed.</errorMessage>
    </validationRules>
